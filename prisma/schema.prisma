generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review {
  id              String   @id @default(cuid())
  rating          Int
  reviewText      String
  userReply       String
  summary         String
  recommendedNext String
  status          String   @default("completed")
  createdAt       DateTime @default(now())
}

